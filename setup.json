{
  #Path to the config for iris to use.
  "graph_config_path" : "/wksp/test/iris_test/graph_config",

  #Whether or not to output graph timings to log.
  "graph_timing_enable" : false,

  #The path to the modules for iris to load.
  "module_path"      : "/wksp/test/iris_test/modules",

  #Whether or not logging should be enabled.
  "log_enable"        : true,

  #The mode for the logging to use when outputting logs.
  "log_mode"          : "Verbose",

  #The path to log output.
  "log_output_path"   : "/var/tmp",

  #Whether or not logging should output to standard out or not.
  "log_use_stdout"    : true
}
